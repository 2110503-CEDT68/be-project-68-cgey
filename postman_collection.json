        {
            "name": "Step 0 - Setup: Create Company 1",
            "event": [
                {
                    "listen": "prerequest",
                    "script": {
                        "exec": [
                            "// First login as admin to get token",
                            "pm.sendRequest({",
                            "    url: pm.collectionVariables.get('BASE_URL') + '/login',",
                            "    method: 'POST',",
                            "    header: { 'Content-Type': 'application/json' },",
                            "    body: {",
                            "        mode: 'raw',",
                            "        raw: JSON.stringify({",
                            "            email: pm.collectionVariables.get('ADMIN_EMAIL'),",
                            "            password: pm.collectionVariables.get('ADMIN_PASSWORD')",
                            "        })",
                            "    }",
                            "}, function (err, res) {",
                            "    const json = res.json();",
                            "    pm.collectionVariables.set('ADMIN_TOKEN', json.data.token);",
                            "});"
                        ]
                    }
                },
                {
                    "listen": "test",
                    "script": {
                        "exec": [
                            "const res = pm.response.json();",
                            "pm.test('Company 1 created successfully', function () {",
                            "    pm.expect(pm.response.code).to.equal(201);",
                            "    pm.expect(res.success).to.be.true;",
                            "    pm.expect(res.data.name).to.equal('Tech Corp');",
                            "});",
                            "pm.collectionVariables.set('COMPANY_ID', res.data._id);"
                        ]
                    }
                }
            ],
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    },
                    {
                        "key": "Authorization",
                        "value": "Bearer {{ADMIN_TOKEN}}"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n  \"name\": \"Tech Corp\",\n  \"address\": \"123 Silicon Valley, CA 94043\",\n  \"website\": \"https://techcorp.com\",\n  \"description\": \"A leading technology company specializing in software development\",\n  \"telephone\": \"02-111-1111\"\n}"
